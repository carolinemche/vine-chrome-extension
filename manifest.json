{
  "name": "Vine Box",
  "manifest_version": 2,
  "version": "0.1",

  "description": "a chrome extension that plays a vine",

  "browser_action": {
    "default_icon": "vine.png",
    "default_title": "Watch a Vine!",
    "default_popup": "vineScreen.html"
  },
  "permissions": ["<all_urls>", "activeTab"]
}
